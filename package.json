{
  "name": "oclif-dynamic-commands",
  "version": "0.0.0",
  "description": "oclif plugin that loads commands dynamically from a directory.",
  "author": "Dan Hulton @DanHulton",
  "bin": {
    "oclif-dynamic-commands": "./bin/run"
  },
  "homepage": "https://github.com/DanHulton/oclif-dynamic-commands",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "DanHulton/oclif-dynamic-commands",
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.17",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.25",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "oclif-dynamic-commands",
    "dirname": "oclif-dynamic-commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "hooks": {
      "init": [
        "./lib"
      ]
    }
  },
  "scripts": {
    "build": "shx rm -rf lib && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/lib"
  ],
  "bugs": "https://github.com/DanHulton/oclif-dynamic-commands/issues",
  "keywords": [
    "oclif"
  ],
  "types": "lib/index.d.ts"
}
